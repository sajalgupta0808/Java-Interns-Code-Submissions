public class BinaryTree {

	public static void main(String[] args) {
	
		Node node = new Node(2);
		node.left = new Node(1);
		node.right = new Node(3);

	}
	
	static class Node{
		
		int value;
		
		Node left, right; 
		
		Node(int value){
			this.value = value;
			
			left = null;
			right =  null;
		}
	}

}